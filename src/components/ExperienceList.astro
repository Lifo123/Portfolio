---

interface Props {
    place: string;
    position: string;
    description: string;
    date: string;

}

const props = Astro.props;
---

<div class="exp-list f-row g-3 relative ml-4">
    <span class="circle br-50 absolute"></span>
    <div class="f-col ml-1">
        <h3 class="fs-4 fw-700">{props.position}</h3>
        <p>{props.date}</p>
    </div>
</div>
