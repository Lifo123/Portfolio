---
import '@Assets/css/Global.css'
---

<head>
    <link
        rel="stylesheet"
        href="https://cdn.jsdelivr.net/gh/Lifo123/FlifoCSS@main/src/output/Flifo.css"
    />
</head>
<div class="fixed f-col f-center h-100 w-100">
    <div class="f-col g-1 f-center mb-12">
        <h1>Page Not Found</h1>
        <h2 class="fs-5">Error 404</h2>
        <p
            class="btn-black br-max d-flex mt-4 mb-12 fw-700 fs-2 pointer"
            onclick="history.back()"
        >
            Get Back
        </p>
    </div>
</div>

<style is:inline>
    p {
        padding: 12px 24px;
        width: max-content;
        background-color: var(--f-tittle);
        color: var(--f-bg);
    }
</style>
